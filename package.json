      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "tsup": "^8.1.0",
    "tsx": "^4.15.1",
    "typescript": "^5"
  }
}
