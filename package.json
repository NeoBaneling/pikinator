{
  "name": "pikinator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --minify",
